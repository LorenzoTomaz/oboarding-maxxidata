    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "nivo": "^0.31.0",
    "nprogress": "^0.2.0",
    "password-validator": "^5.0.2",
    "prop-types": "^15.6.2",
    "query-string": "^6.8.3",
    "react": "^16.4.1",
    "react-apexcharts": "^1.3.7",
    "react-beautiful-dnd": "^13.0.0",
