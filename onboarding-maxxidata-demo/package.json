{
  "name": "onboarding-maxxidata-demo",
  "version": "0.1.24-rc",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired --env=jsdom",
    "qa": "PORT=8081 react-scripts start",
    "lint": "xo --quiet",
    "lint:fix": "xo --quiet --write src/**/*.js",
    "format:root": "prettier --write src/*.js",
    "format:primary": "prettier --write src/**/*.{js,jsx}",
    "format:secondary": "prettier --write src/**/**/*.{js,jsx}",
    "format:third": "prettier --write src/**/**/**/*.{js,jsx}",
    "format": "yarn format:root && yarn format:primary && yarn format:secondary && yarn format:third"
  },
